[Configuration]
LastActiveMode=THUNAR_RENAMER_MODE_NAME
LastActiveRenamer=ThunarSbrInsertRenamer

[ThunarSbrCaseRenamer]
Mode=THUNAR_SBR_CASE_RENAMER_MODE_LOWER

[ThunarSbrRemoveRenamer]
EndOffsetMode=THUNAR_SBR_OFFSET_MODE_LEFT
StartOffset=0
EndOffset=90
StartOffsetMode=THUNAR_SBR_OFFSET_MODE_LEFT

[ThunarSbrReplaceRenamer]
Regexp=TRUE
Replacement=john_
CaseSensitive=FALSE
Pattern=1_

[ThunarSbrNumberRenamer]
TextMode=THUNAR_SBR_TEXT_MODE_TN
Mode=THUNAR_SBR_NUMBER_MODE_000100020003
Start=1
Text=foto_

[ThunarSbrInsertRenamer]
Offset=9
Mode=THUNAR_SBR_INSERT_MODE_INSERT
OffsetMode=THUNAR_SBR_OFFSET_MODE_LEFT
Text=.jpg

